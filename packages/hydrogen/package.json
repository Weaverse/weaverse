{
  "version": "1.0.60",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --outDir dist --watch --external react",
    "build": "tsup src/index.tsx --format esm,cjs --dts --outDir dist --external react",
    "clean": "rimraf dist",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true
  },
  "name": "@weaverse/hydrogen",
  "author": "Weaverse Team",
  "dependencies": {
    "@shopify/hydrogen": "2023.7.0",
    "@weaverse/react": "1.0.60"
  },
  "devDependencies": {
    "@shopify/remix-oxygen": "^1.1.1"
  }
}
