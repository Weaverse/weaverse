{
  "version": "2.5.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsup src/index.ts --format esm,cjs --dts --outDir dist --watch --external @remix-run/react,@shopify/hydrogen",
    "build": "tsup src/index.ts --format esm,cjs --dts --outDir dist --external @remix-run/react,@shopify/hydrogen",
    "clean": "rimraf dist",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@remix-run/react": "^2.1.0",
    "@shopify/hydrogen": "^2023.10.2"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true
  },
  "name": "@weaverse/hydrogen",
  "author": "Weaverse Team",
  "dependencies": {
    "@weaverse/react": "2.5.1",
    "react-error-boundary": "^4.0.11"
  },
  "devDependencies": {
    "@shopify/remix-oxygen": "^2.0.1"
  }
}
