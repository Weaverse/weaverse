{
  "version": "1.0.38",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --outDir dist --watch --external react,keen-slider --sourcemap",
    "build": "tsup src/index.tsx --format esm,cjs --dts --outDir dist --external react,keen-slider",
    "clean": "rimraf dist",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true
  },
  "name": "@weaverse/shopify",
  "author": "Weaverse Team",
  "dependencies": {
    "@weaverse/react": "1.0.38",
    "clsx": "^1.2.1",
    "react-error-boundary": "^4.0.4"
  }
}
