{
  "name": "@weaverse/schema",
  "version": "0.4.0",
  "description": "Schema definitions for Weaverse",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "outDir": "dist"
  },
  "author": "Weaverse",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weaverse/weaverse"
  },
  "dependencies": {
    "zod": "3.25.55"
  }
}
