{
  "version": "2.2.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsup src/index.ts --format esm,cjs --dts --outDir dist --watch --external react,clsx --sourcemap",
    "build": "tsup src/index.ts --format esm,cjs --dts --outDir dist --external react,clsx",
    "clean": "rimraf dist",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true
  },
  "name": "@weaverse/react",
  "author": "Weaverse Team",
  "dependencies": {
    "@weaverse/core": "2.2.5"
  }
}
