{
  "version": "0.1.8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --name sdk-react",
    "dev": "tsdx watch --name sdk-react",
    "build": "tsdx build --name sdk-react",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build --name sdk-react",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@weaverse/react",
  "author": "Weaverse Team",
  "module": "dist/sdk-react.esm.js",
  "size-limit": [
    {
      "path": "dist/sdk-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-is": "^17.0.3",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@nextui-org/react": "^1.0.2-beta.3",
    "@weaverse/core": "^0.2.3"
  }
}
