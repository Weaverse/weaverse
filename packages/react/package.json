{
  "name": "@weaverse/react",
  "author": "Weaverse Team",
  "description": "React bindings for Weaverse",
  "version": "5.5.0-beta.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "repository": {
    "url": "git+https://github.com/Weaverse/weaverse.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "access": "public",
    "@weaverse:registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "outDir": "dist"
  },
  "dependencies": {
    "@weaverse/core": "5.5.0-beta.2",
    "clsx": "^2.1.1",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependencies": {
    "react-router": ">=6"
  },
  "devDependencies": {
    "vitest": "^2.0.0"
  }
}
