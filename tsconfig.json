{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "importHelpers": true,
    "strict": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "declaration": true,
    "strictNullChecks": true
  }
}
